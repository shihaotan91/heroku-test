<%# <h2>Add an animal</h2>

<form method="post" action="/animal">

  <div class="form-group">
    <label for="breed">Breed</label>
    <input type="text" class="form-control" id="breed" name="animal[breed]" placeholder="Breed">
  </div>

  <div class="form-group">
    <label for="family">Family</label>
    <input type="text" class="form-control" id="family" name="animal[family]" placeholder="Family">
  </div>

  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" class="form-control" id="name" name="animal[name]" placeholder="Name">
  </div>

  <div class="form-group">
    <label for="gender">Gender</label>
    <input type="text" class="form-control" id="Gender" name="animal[gender]" placeholder="Gender">
  </div>


<div class="form-group">
  <label for="date">Date</label>
  <input type="text" class="form-control" id="date" name="animal[date]" placeholder="Date">

</div>
<button type="submit" class="btn btn-default">Add an Animal</button>
</form>

<br>%>
